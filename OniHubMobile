local service = "OniHub"

local function GenerateLink(service)
    return loadstring(game:HttpGet(('https://pandadevelopment.net/service_api/PandaBetaLib.lua')))():GetLink(service)
end

local function validate_key(service, key)
    return loadstring(game:HttpGet(('https://pandadevelopment.net/service_api/PandaBetaLib.lua')))():ValidateKey(service, key)
end

local getlink = GenerateLink("OniHub")
print(getlink)
setclipboard(getlink)

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

local Frame = Instance.new("Frame")
    Frame.Size = UDim2.new(0, 300, 0, 220)
    Frame.Position = UDim2.new(0.5, -150, 0.5, -110)
    Frame.BackgroundColor3 = Color3.fromRGB(116, 18, 49)
    Frame.BorderSizePixel = 0
    Frame.ClipsDescendants = true 
    Frame.Parent = ScreenGui

    local UICorner = Instance.new("UICorner")
    UICorner.CornerRadius = UDim.new(1.1, 9) 
    UICorner.Parent = Frame

    local KeyBox = Instance.new("TextBox")
    KeyBox.Size = UDim2.new(0.8, 0, 0, 20)
    KeyBox.Position = UDim2.new(0.1, 0, 0.25, 0)
    KeyBox.PlaceholderText = "Paste your key here"
    KeyBox.BackgroundTransparency = 0.5 
    KeyBox.Parent = Frame

    local AuthenticationButton = Instance.new("TextButton")
    AuthenticationButton.Size = UDim2.new(0.8, 0, 0, 20)
    AuthenticationButton.Position = UDim2.new(0.1, 0, 0.50, 0)
    AuthenticationButton.Text = "Check Key"
    AuthenticationButton.BackgroundTransparency = 0.5
    AuthenticationButton.Parent = Frame

    local CloseButton = Instance.new("TextButton")
    CloseButton.Size = UDim2.new(0.8, 0, 0, 20)
    CloseButton.Position = UDim2.new(0.1, 0, 0.70, 0)
    CloseButton.Text = "Close"
    CloseButton.BackgroundTransparency = 0.5 
    CloseButton.Parent = Frame

AuthenticationButton.MouseButton1Click:Connect(function()
    local keyinput = KeyBox.Text
    local Authentication = validate_key("OniHub", keyinput)

    if Authentication then
        print('Successfully Authenticated')
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Phyoue/OniHubPSM/main/onihubps", true))();
        ScreenGui:Destroy()
    else
        warn('Invalid Key')
    end
end)

CloseButton.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)
